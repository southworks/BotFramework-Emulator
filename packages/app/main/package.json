{
  "name": "@bfemulator/main",
  "version": "4.0.15-alpha",
  "private": true,
  "description": "Development tool for the Microsoft Bot Framework. Allows developers to test and debug bots on localhost.",
  "main": "./app/server/main.js",
  "homepage": "https://github.com/Microsoft/BotFramework-Emulator",
  "scripts": {
    "build": "run-s lint build:electron",
    "build:electron": "tsc -p . --outdir app/server",
    "lint": "tslint --project tsconfig.json",
    "start": "concurrently --kill-others --names \"electron,react-app\" --success first \"npm run start:electron:dev\" \"npm run start:react-app\"",
    "start:electron": "./node_modules/.bin/electron --inspect=7777 .",
    "start:electron:dev": "cross-env ELECTRON_TARGET_URL=http://localhost:3000/ npm run start:electron",
    "start:react-app": "cd ../client && npm start",
    "test": "jest",
    "verify": "gulp verify:copyright"
  },
  "keywords": [
    "microsoft",
    "botframework",
    "botbuilder",
    "studio",
    "emulator",
    "bots",
    "chatbot"
  ],
  "author": {
    "name": "Microsoft Corporation",
    "email": "example@example.com",
    "url": "https://github.com/Microsoft/BotFramework-Emulator"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/formidable": "^1.0.30",
    "@types/jest": "^22.2.3",
    "@types/jsonpath": "^0.2.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "8.9.3",
    "@types/request": "^2.47.0",
    "@types/restify": "^5.0.7",
    "@types/restify-cors-middleware": "^1.0.1",
    "@types/semver": "^5.5.0",
    "@types/ws": "^4.0.1",
    "asar-integrity": "^0.2.4",
    "botbuilder": "^3.14.0",
    "chalk": "^2.4.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "electron": "1.8.3",
    "electron-builder": "^20.10.0",
    "electron-builder-http": "^19.27.5",
    "electron-builder-lib": "^20.11.1",
    "electron-builder-squirrel-windows": "^20.11.0",
    "electron-react-devtools": "^0.5.3",
    "fancy-log": "^1.3.2",
