    "gulp-shell": "^0.6.5",
    "gulp-tslint": "8.1.3",
    "gulp-wait": "^0.0.2",
    "jest": "^23.0.0",
    "license-list": "^0.1.3",
    "npm-run-all": "^4.1.3",
    "through2": "^2.0.3",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "2.7.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
